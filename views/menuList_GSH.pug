            script(type='text/javascript').
                function initialize() {
                    var mapLocation = new google.maps.LatLng('36.628747', '127.455257'); // 지도에서 가운데로 위치할 위도와 경도
                    var markLocation = new google.maps.LatLng('36.628747', '127.455257'); // 마커가 위치할 위도와 경도
                    var mapOptions = {
                        center: mapLocation, // 지도에서 가운데로 위치할 위도와 경도(변수)
                        zoom: 18, // 지도 zoom단계
                        mapTypeId: google.maps.MapTypeId.ROADMAP
                    };
                    var map = new google.maps.Map(document.getElementById("resultMap"), // id: map-canvas, body에 있는 div태그의 id와 같아야 함
                        mapOptions);
                    var size_x = 60; // 마커로 사용할 이미지의 가로 크기
                    var size_y = 60; // 마커로 사용할 이미지의 세로 크기
                    // 마커로 사용할 이미지 주소
                    var image = new google.maps.MarkerImage( 'http://www.larva.re.kr/home/img/boximage3.png',
                        new google.maps.Size(size_x, size_y),
                        '',
                        '',
                        new google.maps.Size(size_x, size_y));
                    var marker;
                    marker = new google.maps.Marker({
                        position: markLocation, // 마커가 위치할 위도와 경도(변수)
                        map: map,
                        icon: image, // 마커로 사용할 이미지(변수)
                        //             info: '말풍선 안에 들어갈 내용',
                        title: '충북대학교' // 마커에 마우스 포인트를 갖다댔을 때 뜨는 타이틀
                    });
                    var content = "Marker"; // 말풍선 안에 들어갈 내용
                    // 마커를 클릭했을 때의 이벤트. 말풍선 뿅~
                    var infowindow = new google.maps.InfoWindow({ content: content});
                    google.maps.event.addListener(marker, "click", function() {
                        infowindow.open(map,marker);
                    });
                }
                google.maps.event.addDomListener(window, 'load', initialize);
                //GoogleMap 코드
                $(function(){
                    var mapOffset = $( '.scroll-menu' ).offset();
                    $("#mapChkbox").change(function(){
                        if($("#mapChkbox").is(":checked")){
                            if(document.getElementById('menuPic').classList.contains("col-12")){
                                document.getElementById('menuPic').classList.remove("col-12")
                                document.getElementById('menuPic').classList.add("col-9")
                            }
                            if(document.getElementById('resultMap').classList.contains('mapInvisible')){
                                document.getElementById('resultMap').classList.remove('mapInvisible')
                            }
                            mapOffset = $( '.scroll-menu' ).offset();
                        }else{
                            if(document.getElementById('menuPic').classList.contains("col-9")){
                                document.getElementById('menuPic').classList.remove("col-9")
                                document.getElementById('menuPic').classList.add("col-12")
                            }
                            if(document.getElementById('resultMap').classList.contains('mapInvisible')==0){
                                document.getElementById('resultMap').classList.add('mapInvisible')
                            }
                        }
                    });
                    $(window).scroll(function(){  //스크롤하면 아래 코드 실행
                        if (matchMedia("screen and (min-width: 960px)").matches) {
                            if( $( document ).scrollTop() > mapOffset.top ){
                                $(".scroll-menu").addClass("scroll-fixed");
                            }else{
                                $(".scroll-menu").removeClass("scroll-fixed");
                            }
                        }
                    });
                });
                //Map 스크롤시 고정시키는 JAVASCRIPTS
                $(window).on("resize",function(){
                    $(".lgMapDiv").hide();
                    $(".mapIcon").show();
                    if (!matchMedia("screen and (min-width: 960px)").matches) {
                        if(document.getElementById('menuPic').classList.contains("col-9")){
                            document.getElementById('menuPic').classList.remove("col-9")
                            document.getElementById('menuPic').classList.add("col-12")
                        }
                        if(document.getElementById('resultMap').classList.contains('mapInvisible')==0){
                            document.getElementById('resultMap').classList.add('mapInvisible')
                        }
                        if(document.getElementById('resultMap').classList.contains("col-3")){
                            document.getElementById('resultMap').classList.remove("col-3");
                        }
                        if(!document.getElementById('resultMap').classList.contains("col-10")){
                            document.getElementById('resultMap').classList.add("col-10");
                        }
                        if(document.getElementById('resultMap').classList.contains("scroll-fixed")){
                            $(".scroll-menu").removeClass("scroll-fixed");
                        }
                        if(!document.getElementById('resultMap').classList.contains("mobile-fixed")){
                            $(".scroll-menu").addClass("mobile-fixed");
                        }
                        if($("#mapChkbox").is(":checked")){
                            $("#mapChkbox").attr("checked",false);
                        }
                    }
                    else{
                        $(".mapIcon").hide();
                        $(".lgMapDiv").show();
                        if(document.getElementById('resultMap').classList.contains('mapInvisible')==0){
                            document.getElementById('resultMap').classList.add('mapInvisible')
                        }
                        if(document.getElementById('resultMap').classList.contains("col-10")){
                            document.getElementById('resultMap').classList.remove("col-10");
                            document.getElementById('resultMap').classList.add("col-3");
                        }
                        if(document.getElementById('resultMap').classList.contains("mobile-fixed")){
                            $(".scroll-menu").removeClass("mobile-fixed");
                        }
                    }
                }).resize();
                $(".mapIcon").click(function(){
                    if(document.getElementById('resultMap').classList.contains('mapInvisible')){
                        document.getElementById('resultMap').classList.remove('mapInvisible');
                    }
                    else{
                        document.getElementById('resultMap').classList.add('mapInvisible');
                    }
                });
